{
  "name": "node-rdkafka-acosom",
  "version": "v2.16.1",
  "description": "Node.js bindings for librdkafka",
  "librdkafka": "2.2.0",
  "main": "lib/index.js",
  "scripts": {
    "configure": "node-gyp configure",
    "build": "node-gyp build",
    "build:dev": "node-gyp build -j 8 --debug",
    "test": "make test",
    "test:e2e": "npm run test:e2eOld && npm run test:e2eNew",
    "test:e2eOld": "make e2e",
    "test:e2eNew": "tap testsNew/e2e",
    "install": "node-gyp rebuild",
    "prepack": "node ./ci/prepublish.js"
  },
  "keywords": [
    "kafka",
    "librdkafka"
  ],
  "repository": {
    "type": "git",
    "url": "git:@github.com/acosom/node-rdkafka-acosom.git"
  },
  "contributors": [
    {
      "name": "Robin Fehr",
      "email": "contact@acosom.com"
    },
    {
      "name": "Leonard Ikle",
      "email": "contact@acosom.com"
    }
  ],
  "tap": {
    "coverage-map": "map.js"
  },
  "license": "MIT",
  "devDependencies": {
    "bluebird": "^3.5.3",
    "jsdoc": "^3.4.0",
    "jshint": "^2.10.1",
    "mocha": "^5.2.0",
    "node-gyp": "^9.4.0",
    "toolkit-jsdoc": "^1.0.0"
  },
  "dependencies": {
    "bindings": "^1.3.1",
    "nan": "^2.18.0",
    "tap": "^16.3.4"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}